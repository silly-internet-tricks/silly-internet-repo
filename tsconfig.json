{
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "src/server/"
  ],
  "compilerOptions": {
    "outDir": "./ts-compiled/",
    "noImplicitAny": true,
    "module": "es2022",
    "target": "es2022",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
  }
}