{
  "include": [
    "*.ts"
  ],
  "compilerOptions": {
    "outDir": "../../ts-compiled/server/",
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "es2020",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}